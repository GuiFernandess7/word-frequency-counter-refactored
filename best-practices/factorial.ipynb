   "source": [
    "def Factorial(num):\n",
    "    factorial = 1\n",
    "\n",
    "    for n in range(1, num + 1):\n",
    "        factorial *= n\n",
    "\n",
    "    return factorial\n",
    "\n",
    "assert Factorial(1) == 1\n",
    "assert Factorial(4) == 24"
   ]
  },
